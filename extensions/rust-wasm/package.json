{
  "name": "@matterviz/wasm",
  "version": "0.0.1",
  "description": "WASM bindings for ferrox structure matching library",
  "type": "module",
  "main": "index.js",
  "types": "types.d.ts",
  "exports": {
    ".": {
      "types": "./types.d.ts",
      "import": "./index.js"
    },
    "./ferrox_bg.wasm": "./pkg/ferrox_bg.wasm"
  },
  "files": [
    "index.js",
    "types.d.ts",
    "pkg/"
  ],
  "scripts": {
    "build": "wasm-pack build ../rust --target web --features wasm --no-default-features --out-dir ../rust-wasm/pkg",
    "build:dev": "wasm-pack build ../rust --target web --features wasm --no-default-features --out-dir ../rust-wasm/pkg --dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janosh/matterviz.git",
    "directory": "extensions/rust-wasm"
  },
  "keywords": [
    "wasm",
    "crystallography",
    "structure-matching",
    "materials-science"
  ],
  "author": "Janosh Riebesell <janosh@lbl.gov>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janosh/matterviz/issues"
  },
  "homepage": "https://github.com/janosh/matterviz#readme"
}
